    <!--<item name="searchSuggestionIcon">@drawable/ic_suggestion</item>-->

    <!--&lt;!&ndash; Change background for the suggestions list view &ndash;&gt;-->
    <!--<item name="searchSuggestionBackground">@android:color/white</item>-->

    <!--&lt;!&ndash; Change text color for edit text. This will also be the color of the cursor &ndash;&gt;-->
    <!--&lt;!&ndash;<item name="android:textColor">@color/theme_primary_text_inverted</item>&ndash;&gt;-->

    <!--&lt;!&ndash; Change hint text color for edit text &ndash;&gt;-->
    <!--&lt;!&ndash;<item name="android:textColorHint">@color/theme_secondary_text_inverted</item>&ndash;&gt;-->

    <!--&lt;!&ndash; Hint for edit text &ndash;&gt;-->
    <!--<item name="android:hint">@string/search_hint</item>-->
    <!--</style>-->

    <style name="MyProgress">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@android:drawable/progress_horizontal</item>
    </style>

    <style name="person_info_check_rectangle_linear_layout">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="left_info_text_style">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="right_info_text_style">
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:singleLine">true</item>
    </style>

</resources>